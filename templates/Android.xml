<templateSet group="Android">
  <template name="inv" value="@butterknife.InjectView(R.id.$VIEW_ID$) $VIEW_TYPE$ $VAR_NAME$;" description="InjectView Butterknife" toReformat="false" toShortenFQNames="true">
    <variable name="VIEW_ID" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <variable name="VIEW_TYPE" expression="classNameComplete()" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR_NAME" expression="suggestVariableName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="bt" value="butterknife.ButterKnife.bind($TARGET$);" description="ButterKnife (inject)" toReformat="false" toShortenFQNames="true">
    <variable name="TARGET" expression="&quot;this&quot;" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="oncl" value="@butterknife.OnClick(R.id.$RESOURCE_ID$)&#10;public void on$BUTTON_NAME$Clicked() {&#10;    $END$&#10;}" description="ButterKnife (onClick" toReformat="true" toShortenFQNames="true">
    <variable name="RESOURCE_ID" expression="completeSmart()" defaultValue="" alwaysStopAt="true" />
    <variable name="BUTTON_NAME" expression="&quot;ButtonName&quot;" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="mainl" value="Timber.d(&quot;[MainThread: %b] $END$&quot;, Looper.getMainLooper() == Looper.myLooper());" description="Check if running on Main thread" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="tv" value="Timber.v($END$);" description="Timber verbose log" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="visible" value="$VIEW$.setVisibility(View.VISIBLE);" description="Set view visibility to VISIBLE" toReformat="false" toShortenFQNames="true">
    <variable name="VIEW" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="bv" value="@butterknife.Bind(R.id.$VIEW_ID$) $VIEW_TYPE$ $VAR_NAME$;" description="ButterKnife bind view" toReformat="false" toShortenFQNames="true">
    <variable name="VIEW_ID" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VIEW_TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="editmode" value="if (isInEditMode()){&#10;  return;&#10;}&#10;" description="layout preview return if ineditmode" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>